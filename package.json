{
  "name": "ddm-date-range-input",
  "version": "4.0.0",
  "description": "React Date Range Input and Picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/deseretdigital-ui/date-range-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "date range",
    "date picker"
  ],
  "author": "Russell Ahlstrom <russell.ahlstrom@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "dist",
    "example": "example"
  },
  "scripts": {
    "build": "yarn run build-js; yarn run build-css; yarn run build-example",
    "build-js": "babel ./modules -d lib",
    "build-css": "node-sass --output-style compressed --include-path scss scss/index.scss lib/styles.css",
    "build-example": "webpack",
    "watch-example": "webpack --watch",
    "lint": "eslint modules/*",
    "init": "yarn install --ignore-engines --pure-lockfile"
  },
  "dependencies": {
    "airbnb-browser-shims": "^3.2.1",
    "classnames": "^2.2.6",
    "moment": "^2.18.1",
    "moment-range": "^4.0.2",
    "react-dates": "^20.1.0",
    "react-with-direction": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "react": "^16.3.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "peerDependencies": {
    "react": ">=16.3.x"
  }
}
