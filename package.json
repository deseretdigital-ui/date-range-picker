{
  "name": "ddm-date-range-input",
  "version": "4.0.0",
  "description": "React Date Range Input and Picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/deseretdigital-ui/date-range-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "date range",
    "date picker"
  ],
  "author": "Russell Ahlstrom <russell.ahlstrom@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "dist",
    "example": "example"
  },
  "scripts": {
    "build": "yarn run build-js; yarn run build-css",
    "build-js": "babel ./src -d dist",
    "build-css": "node-sass --output-style compressed --include-path scss scss/index.scss dist/styles.css",
    "lint": "eslint src/*",
    "start": "webpack-dev-server --hot"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "airbnb-browser-shims": "^3.2.1",
    "classnames": "^2.2.6",
    "moment": "^2.18.1",
    "moment-range": "^4.0.2",
    "react-dates": "^21.0.1",
    "react-with-direction": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.10",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "peerDependencies": {
    "react": ">=16.8.x"
  }
}
