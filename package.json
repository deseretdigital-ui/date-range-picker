{
  "name": "ddm-date-range-input",
  "version": "1.5.0",
  "description": "React Date Range Input and Picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/deseretdigital-ui/date-range-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "date range"
  ],
  "author": "Russell Ahlstrom <russell.ahlstrom@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "dist",
    "example": "example"
  },
  "scripts": {
    "build": "npm run build-js; npm run build-css; npm run build-example",
    "build-js": "babel ./modules -d lib",
    "build-css": "node-sass --output-style compressed --include-path scss scss/index.scss lib/styles.css",
    "build-example": "webpack",
    "watch-example": "webpack --watch",
    "lint": "eslint modules/*"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "moment": "^2.10.6",
    "moment-range": "^2.0.3",
    "react-daterange-picker": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.0",
    "eslint-plugin-react": "^5.0",
    "node-sass": "^3.7.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0 <= 15.x.x"
  }
}
