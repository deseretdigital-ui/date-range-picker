{
  "name": "ddm-date-range-input",
  "version": "3.0.3",
  "description": "React Date Range Input and Picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/deseretdigital-ui/date-range-picker.git"
  },
  "keywords": [
    "react",
    "react-component",
    "date range",
    "date picker"
  ],
  "author": "Russell Ahlstrom <russell.ahlstrom@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "dist",
    "example": "example"
  },
  "scripts": {
    "build": "yarn run build-js; yarn run build-css; yarn run build-example",
    "build-js": "babel ./modules -d lib",
    "build-css": "node-sass --output-style compressed --include-path scss scss/index.scss lib/styles.css",
    "build-example": "webpack",
    "watch-example": "webpack --watch",
    "lint": "eslint modules/*",
    "init": "yarn install --ignore-engines --pure-lockfile"
  },
  "dependencies": {
    "airbnb-browser-shims": "^2.1.0",
    "classnames": "^2.2.5",
    "moment": "^2.22.1",
    "moment-range": "^4.0.0",
    "react-dates": "^12.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "node-sass": "^4.8.3",
    "react": "^16.3.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "peerDependencies": {
    "react": ">=16.3.x"
  }
}
